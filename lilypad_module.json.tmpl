{
  "machine": {
    "gpu": 1,
    "cpu": 1000,
    "ram": 100
  },
  "job": {
    "APIVersion": "V1beta1",
    "Spec": {
      "Deal": {
        "Concurrency": 1
      },
      "Docker": {
        "Entrypoint": [
          "python", "-u", "sdxl_inference.py"
        ],
        "Image": "stabilityai/sdxl:latest"
      },
      "Engine": "Docker",
      "Network": {
        "Type": "None"
      },
      "PublisherSpec": {
        "Type": "IPFS"
      },
      "Resources": {
        "GPU": "1"
      },
      "Timeout": 3600,
      "Verifier": "Noop",
      "Inputs": [
        {
          "Name": "prompt",
          "Description": "Text prompt for image generation",
          "Default": "A beautiful landscape with mountains and a lake"
        },
        {
          "Name": "num_images",
          "Description": "Number of images to generate",
          "Default": "1"
        }
      ],
      "Outputs": [
        {
          "Name": "output",
          "Path": "/outputs"
        }
      ]
    }
  }
}
