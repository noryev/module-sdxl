{
    "machine": {
        "gpu": 1,
        "cpu": 1000,
        "ram": 16000
    },
    "job": {
        "APIVersion": "V1beta1",
        "Metadata": {
            "Name": "sdxl-job"
        },
        "Spec": {
            "Deal": {
                "Concurrency": 1
            },
            "Docker": {
                "Entrypoint": null,
                "EnvironmentVariables": [
                    {{ if .Prompt }}"PROMPT={{ .Prompt }}",{{ end }}
                    {{ if .Steps }}"STEPS={{ .Steps }}",{{ end }}
                    {{ if .Seed }}"SEED={{ .Seed }}",{{ end }}
                    {{ if .Sampler }}"SAMPLER={{ .Sampler }}",{{ end }}
                    {{ if .Scheduler }}"SCHEDULER={{ .Scheduler }}",{{ end }}
                    {{ if .Size }}"SIZE={{ .Size }}",{{ end }}
                    {{ if .Batching }}"BATCHING={{ .Batching }}",{{ end }}
                    "HF_HUB_OFFLINE=1"
                ],
                "Image": "sdxl-pipeline:v1.0-base-lilypad3"
            },
            "Engine": "Docker",
            "Network": {
                "Type": "None"
            },
            "Outputs": [
                {
                "Name": "outputs",
                "Path": "/outputs"
                }
            ],
            "PublisherSpec": {
                "Type": "ipfs"
            },
            "Resources": {
                "GPU": "1"
            },
            "Timeout": 1800
        }
    }
}
